		md[3] = new Dog("Green");
		md[4] = new Dog("Blue");
		
		switch (dcolor) {
		    case "Red": md[0].
		    case "Orange":
		    case "Yellow":
		    case "Green":
		    case "Blue":
		}
	}

}
