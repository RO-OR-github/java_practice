package week2_4;

import java.util.Scanner;

public class ActiveCellPhone {
	public static void main(String[] args) {
		boolean powerswitch = true;
		String pnum;
		
		Scanner sc = new Scanner(System.in);
		pnum=sc.next();
		
		CellPhone cp = new CellPhone(pnum,powerswitch);
		cp.powerToggle();
		cp.powerToggle();
		cp.getCount
	}

}

