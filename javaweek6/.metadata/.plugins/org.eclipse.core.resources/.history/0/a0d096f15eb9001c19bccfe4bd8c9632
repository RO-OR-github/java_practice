package javaweek6practice_5;

public class Person {
	private int name;
	private int age;
	protected int num;
	
	public Person() {}
	public Person(int name, int age) {
		this.name = name;
		this.age = age;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + "]";
	}
	
	
	
}
