package javaweek6practice_2;

public class SmartPhone {
	public void turnOn() {
		
	}
	public void turnOff() {
		
	}
	public void setVolume(int volume) {
		
	}
	
	public void setMute(boolean mute) {
		if(mute) {
			System.out.println("무음 처리합니다.");
		} else {
			System.out.println("무음 해제합니다.");
		}
	}

}
