package javaweek5_3;

public class Circle extends Shape {
	private double PI;
	private int r;
	
	public Circle() {}
	public Circle(String fig, int r, double PI) {
		super(fig);
		this.r=r;
		this.PI=PI;
	}
	@Override
	double getArea() {//추상클래스라도 매개변수는 동일하게
		double area = r*r*PI
		return area;
	}

}

