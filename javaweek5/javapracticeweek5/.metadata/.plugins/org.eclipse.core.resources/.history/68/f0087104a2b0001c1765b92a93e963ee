package javaweek5_3;

public class Circle extends Shape {
	final double PI =3.14;
	private int r;
	
	public Circle() {}
	public Circle(String fig, int r, double Pi) {
		super(fig);
		this.x=x;
		this.y=y;
	}
	@Override
	double getArea() {//추상클래스라도 매개변수는 동일하게
		double area = x*y/2;
		return area;
	}

}

