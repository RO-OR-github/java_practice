package javaweek5_2;

public class SmartPhone extends Phone{ //惑加 包拌 结林扁
	private int memory;
	private boolean hasBluetooth;
	public 
	public SmartPhone(String maker, String type, int price, int memory, boolean hasBluetooth) {
		super(maker, type, price);
		this.memory = memory;
		this.hasBluetooth = hasBluetooth;
	}
	public int getMemory() {
		return memory;
	}
	public void setMemory(int memory) {
		this.memory = memory;
	}
	public boolean isHasBluetooth() {//boolean getter
		return hasBluetooth;
	}
	public void setHasBluetooth(boolean hasBluetooth) {
		this.hasBluetooth = hasBluetooth;
	}
	
	
}
