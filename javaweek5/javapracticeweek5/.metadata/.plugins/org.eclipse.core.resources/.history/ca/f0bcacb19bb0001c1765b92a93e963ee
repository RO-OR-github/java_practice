package javaweek5_2;

public class Phone {
	private String maker, type;
	private int price;
	
	public Phone(String maker, String type, int price) {
		this.maker = maker;
		this.type = type;
		this.price = price;
	}

	public String getMaker() {
		return maker;
	}

	public void setMaker(String maker) {
		this.maker = maker;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	

}
