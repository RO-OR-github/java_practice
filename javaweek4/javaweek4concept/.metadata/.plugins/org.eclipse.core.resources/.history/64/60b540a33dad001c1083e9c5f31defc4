package java2conceptweek4_8;

public class Account {
	private String name, money, ccount;

	public Account(String name, String money, String ccount) {
		this.name = name;
		this.money = money;
		this.ccount = ccount;
	}
	
	public void add(int nm) {
		money = money + nm;
		
	}

	@Override
	public String toString() {
		return "계좌번호 : " + ccount + "\n예금주 : " + name + "\n잔액 : " + money + "\n카드번호 : "+ccount;   
	}
	
	

}
