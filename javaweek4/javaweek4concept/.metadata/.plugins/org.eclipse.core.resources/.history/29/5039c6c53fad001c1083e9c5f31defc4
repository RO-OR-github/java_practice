package java2conceptweek4_8;

public class CheckingAccount extends Account {
	private String cardNo;

	public CheckingAccount(String name, int money, String ccount, String card) {
		super(name, money, ccount);
		this.card = cardNo;
	}
	public void milater() {
		super.add(0);
	}
	public void molater() {
		super.sub(0);
	}
	public void pay() {
		super.sub(0);
	
	}
	

}
